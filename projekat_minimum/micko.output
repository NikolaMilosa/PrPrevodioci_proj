   69 $@15: %empty

   70 $@16: %empty

   71 function_call: _ID $@15 _LPAREN $@16 argument _RPAREN

   72 argument: %empty
   73         | num_exp
   74         | argument _COMMA num_exp

   75 if_statement: if_part
   76             | if_part _ELSE statement

   77 @17: %empty

   78 $@18: %empty

   79 if_part: _IF _LPAREN @17 rel_exp $@18 _RPAREN statement

   80 rel_exp: num_exp _RELOP num_exp

   81 return_statement: _RETURN num_exp _SEMICOLON
   82                 | _RETURN _SEMICOLON

   83 select_statement: _SELECT arg_select _FROM _ID _WHERE _LPAREN rel_exp _RPAREN _SEMICOLON

   84 arg_select: _ID
